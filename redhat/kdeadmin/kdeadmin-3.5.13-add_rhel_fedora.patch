+sub check_rhel
+{
+  open RELEASE, "$gst_prefix/etc/redhat-release" or return -1;
+  while (<RELEASE>)
+  {
+    chomp;
+    if (
+         (/^CentOS release (\d)([\d\.]+)\s+.*/)
+      or (/^Scientific Linux release (\d)([\d\.]+)\s+.*/)
+      or (/^Red Hat Enterprise Linux \w release (\d)([\d\.]+)\s+.*/)
+    )
+    {
+      close RELEASE;
+      return "rhel-$1";
+    }
+  }
+  close RELEASE;
+  return -1;
+}
+
