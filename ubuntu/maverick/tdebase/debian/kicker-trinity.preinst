#! /bin/sh -e

case "$1" in

    upgrade)
	rm -f /etc/menu-methods/tdebase
	rm -f /etc/trinity/debian/kde-update-menu.sh
	rm -f /etc/menu-methods/kicker
    ;;

    install)
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;

esac

#DEBHELPER#

exit 0
